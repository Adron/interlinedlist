{
  "name": "interlinedlist",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "vercel-build": "prisma migrate deploy && next build",
    "db:migrate": "node scripts/safe-migrate.js",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:force": "dotenv -e .env.local -- prisma migrate dev",
    "db:generate": "dotenv -e .env.local -- prisma generate",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "backup": "node scripts/backup-database.js",
    "restore": "node scripts/restore-database.js",
    "test-data:seed": "dotenv -e .env.local -- node test-data/seed-test-accounts.js",
    "cli:test": "make -C cli test",
    "cli:test-local": "node scripts/cli-test-local.js",
    "cli:build": "make -C cli all && mkdir -p public/downloads/darwin-arm64 public/downloads/darwin-amd64 public/downloads/linux-arm64 public/downloads/linux-amd64 public/downloads/windows && cp cli/dist/il-sync-darwin-arm64 public/downloads/darwin-arm64/il-sync && cp cli/dist/il-sync-darwin-amd64 public/downloads/darwin-amd64/il-sync && cp cli/dist/il-sync-linux-arm64 public/downloads/linux-arm64/il-sync && cp cli/dist/il-sync-linux-amd64 public/downloads/linux-amd64/il-sync && cp cli/dist/il-sync-windows-amd64.exe public/downloads/windows/il-sync.exe"
  },
  "dependencies": {
    "@atproto/oauth-client-node": "^0.3.16",
    "@prisma/client": "^5.19.0",
    "@types/react-datepicker": "^6.2.0",
    "@uiw/react-md-editor": "^4.0.11",
    "@vercel/blob": "^2.2.0",
    "bcryptjs": "^3.0.3",
    "bootstrap": "^5.3.3",
    "compromise": "^14.11.0",
    "elkjs": "^0.11.0",
    "gray-matter": "^4.0.3",
    "gulp": "^5.0.1",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-datepicker": "^9.1.0",
    "react-dom": "^18.3.0",
    "react-markdown": "^10.1.0",
    "reactflow": "^11.11.4",
    "resend": "^6.6.0",
    "sass": "^1.97.1",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "prisma": "^5.19.0",
    "typescript": "^5.5.0"
  }
}
